<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.innoplayer.testapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InnoPlayerTest">
        <meta-data
            android:name="com.google.android.gms.cast.framework.OPTIONS_PROVIDER_CLASS_NAME"
            android:value="co.innoplayer.testapp.OurCastOptionsProvider" />

        <activity
            android:name=".ListChoiceActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VideoPlayerActivity" />
        <activity android:name=".AudioPlayerActivity" />
        <activity android:name=".ExpandedCastControlerActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
    </application>

</manifest>